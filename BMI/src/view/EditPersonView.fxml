<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="225.0" prefWidth="325.0" styleClass="background" stylesheets="@DarkTheme.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.EditPersonViewController">
   <children>
      <GridPane layoutX="14.0" layoutY="19.0" prefHeight="149.0" prefWidth="303.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label fx:id="lblFirstName" text="First Name" />
            <Label fx:id="lblLastName" text="Last Name" GridPane.rowIndex="1" />
            <Label fx:id="lblHeight" text="Height" GridPane.rowIndex="2" />
            <Label fx:id="lblWeight" text="Weight" GridPane.rowIndex="3" />
            <TextField fx:id="txtFirstName" GridPane.columnIndex="1">
               <styleClass>
                  <String fx:value="background" />
                  <String fx:value="label-bright" />
               </styleClass></TextField>
            <TextField fx:id="txtLastName" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <styleClass>
                  <String fx:value="background" />
                  <String fx:value="label-bright" />
               </styleClass></TextField>
            <TextField fx:id="txtHeight" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <styleClass>
                  <String fx:value="background" />
                  <String fx:value="label-bright" />
               </styleClass></TextField>
            <TextField fx:id="txtWeight" GridPane.columnIndex="1" GridPane.rowIndex="3">
               <styleClass>
                  <String fx:value="background" />
                  <String fx:value="label-bright" />
               </styleClass></TextField>
         </children>
      </GridPane>
      <Button fx:id="btnOk" layoutX="78.0" layoutY="185.0" mnemonicParsing="false" onAction="#handleOkButton" text="OK" />
      <Button fx:id="btnCancel" layoutX="208.0" layoutY="185.0" mnemonicParsing="false" onAction="#handleCancelButton" text="Cancel" />
   </children>
</AnchorPane>
